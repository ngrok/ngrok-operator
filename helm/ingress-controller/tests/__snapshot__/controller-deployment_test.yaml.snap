              valueFrom:
                secretKeyRef:
                  key: AUTHTOKEN
                  name: RELEASE-NAME-kubernetes-ingress-controller-credentials
            - name: POD_NAMESPACE
              valueFrom:
                fieldRef:
                  fieldPath: metadata.namespace
